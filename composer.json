{
    "name": "etrias/magento-connector",
    "description": "",
    "require": {
        "php": ">=7.1",
        "ext-soap": "*",
        "phpro/soap-client": "^0.5.0",
        "http-interop/http-factory": "^0.2.0",
        "guzzlehttp/guzzle": "^6.2",
        "http-interop/http-factory-guzzle": "^0.1.0",
        "symfony/cache": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\MagentoConnector\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Etrias\\MagentoConnector\\": "./tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpunit/phpunit": "^6.2",
        "zendframework/zend-code": "^3.0.4"
    },
    "scripts": {
        "fix-style": [
            "vendor/bin/php-cs-fixer fix --diff --show-progress run-in"
        ]
    },
    "authors": [
        {
            "name": "Christ-Jan",
            "email": "christ-jan@etrias.nl"
        }
    ],
    "minimum-stability": "stable"
}
